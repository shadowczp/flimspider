create database spider;
use spider;
create table dy8c_title_item(
id varchar(20) primary key,
detail_url varchar(50),
update_time date,
title varchar(100),
note varchar(80),
img_url varchar(200)
);
create table dy8c_detail_item(
id varchar(20),
url_info varchar(200),
url varchar(2000),
content_size varchar(30)
);